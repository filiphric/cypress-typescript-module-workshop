{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "es5",
      "dom"
    ],
    "types": [
      "cypress",
      "node"
    ],
    "paths": {
      "@typings/*": [
        "cypress/support/typings/*"
      ],
      "@commands/*": [
        "cypress/support/commands/*"
      ]
    },
    "baseUrl": "./",
    "allowJs": false,
    "resolveJsonModule": true
  },
  "include": [
    "cypress/**/*.ts"
  ]
}